<%- include('../partials/header') %>

<main class="main" id="main">
    <% data.forEach(item=>{ %>
        <p class="id"><%= item.id %></p>
        <p class="name"><%= item.name %></p>
        <img src="<%= item.image %> " alt="image of <%= item.name %>">
    <% }) %> 
</main>

<%- include('../partials/footer') %> 